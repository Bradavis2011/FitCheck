{
  "expo": {
    "name": "Or This?",
    "slug": "orthis",
    "scheme": "orthis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBF7F4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bradavis.orthis",
      "infoPlist": {
        "NSCameraUsageDescription": "Or This? uses your camera to analyze your outfit and provide AI-powered style feedback.",
        "NSPhotoLibraryUsageDescription": "Or This? accesses your photo library so you can select outfit photos for analysis.",
        "NSPhotoLibraryAddUsageDescription": "Or This? saves outfit score cards to your photo library.",
        "NSMicrophoneUsageDescription": "Or This? may use the microphone when recording video outfits.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.bradavis.orthis",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FBF7F4"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3940256099942544~3347511713",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd6e74d0-ce99-492d-aad3-ada5bad060d4"
      }
    },
    "owner": "outfitcheck"
  }
}
