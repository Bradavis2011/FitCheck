{
  "expo": {
    "name": "Or This?",
    "slug": "orthis",
    "scheme": "orthis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FBF7F4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bradavis.orthis"
    },
    "android": {
      "package": "com.bradavis.orthis",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FBF7F4"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-dev-client"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c98d9cbc-caf8-4853-9bc9-c9485ad17f36"
      }
    }
  }
}
